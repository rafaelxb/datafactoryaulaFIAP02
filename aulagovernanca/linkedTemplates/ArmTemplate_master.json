{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aulagovernanca"
		},
		"AzureDataLakeStorageAula_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageAula'"
		},
		"AzureMySqlSakila_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySqlSakila'"
		},
		"MySqlCarloca_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySqlCarloca'"
		},
		"AzureDataLakeStorageAula_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samappingrafaelx.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aulagovernanca_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageAula_accountKey": {
						"value": "[parameters('AzureDataLakeStorageAula_accountKey')]"
					},
					"AzureMySqlSakila_connectionString": {
						"value": "[parameters('AzureMySqlSakila_connectionString')]"
					},
					"MySqlCarloca_connectionString": {
						"value": "[parameters('MySqlCarloca_connectionString')]"
					},
					"AzureDataLakeStorageAula_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageAula_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}